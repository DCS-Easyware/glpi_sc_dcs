        <div class="header">{{ firmware.name }}</div>
        <div class="meta"><span class="ui {{ firmware.color }} text">{{ translation.firmware }}</span></div>
      </div>
      <div class="content">
        <div class="ui relaxed list">
          <div class="item">
            <div class="content">
              <a class="header">{{ translation.manufacturer }}</a>
              <div class="description">{{ firmware.manufacturer }}</div>
            </div>
          </div>
          <div class="item">
            <div class="content">
