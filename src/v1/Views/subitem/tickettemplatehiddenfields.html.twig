{{ include('header.html.twig') }}
{{ include('headerForm.html.twig') }}

<table class="ui blue table">
  <thead>
    <tr>
      <th>{{ translation.name }}</th>
    </tr>
  </thead>
  <tbody>
    {% for hiddenfield in data.hiddenfields|sort((a, b) => a.hiddenfield.name|lower <=> b.hiddenfield.name|lower) %}
      <tr>
        <td><a>{{ hiddenfield.name }}</a></td>
      </tr>
    {% endfor %}
  </tbody>
</table>

