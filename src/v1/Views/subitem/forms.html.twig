{% if data.src == 'section' %}
  <table class="ui blue table">
    <thead>
      <tr>
        <th>{{ translation.name }}</th>
      </tr>
    </thead>
    <tbody>
      {% for form in data.forms %}
        <tr>
          <td><a href="{{ data.rootUrl2 }}/forms/{{ form.id }}">{{ form.name }}</a></td>
        </tr>
      {% endfor %}
    </tbody>
  </table>
{% endif %}

