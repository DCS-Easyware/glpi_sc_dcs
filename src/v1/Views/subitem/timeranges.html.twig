<table class="ui blue table">
  <thead>
    <tr>
      <th>{{ translation.name }}</th>
      <th>{{ translation.begin }}</th>
      <th>{{ translation.end }}</th>
    </tr>
  </thead>
  <tbody>
    {% for key,timerange in data.timeranges %}
      <tr>
        <td>{{ timerange.name }}</td>
        <td>{{ timerange.begin }}</td>
        <td>{{ timerange.end }}</td>
      </tr>
    {% endfor %}
  </tbody>
</table>

